FROM flowable/flowable-rest:latest

# The base image explodes the WAR under /app. Extra libs are picked up from /app/WEB-INF/lib.
# Build the module first: mvn -pl flowable-rest-keycloak-security -am clean package
COPY flowable-rest-keycloak-security/target/flowable-rest-keycloak-security.jar /app/WEB-INF/lib/flowable-rest-keycloak-security.jar
