FROM flowable/flowable-rest:latest

# The base image loads any additional jars placed under /app/libs on the classpath.
# Build the module first: mvn -pl flowable-rest-keycloak-security -am clean package
COPY flowable-rest-keycloak-security/target/flowable-rest-keycloak-security.jar /app/libs/flowable-rest-keycloak-security.jar
